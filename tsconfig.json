{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "moduleResolution": "node",
    "paths": {
      "_assets": ["src/assets/*"],
      "_components": ["src/components/index.tsx"],
      "_navigations/*": ["src/navigations/*"],
      "_styles": ["src/styles/index.ts"],
